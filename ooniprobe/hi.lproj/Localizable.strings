"risks_text_1" = "यूनीप्रोबे स्वतंत्र और खुला स्रोत सॉफ्टवेयर है कि आप अपने नेटवर्क में इंटरनेट सेंसरशिप की जांच करने के लिए सक्षम बनाता है!";
"risks_text_2" = "इस एप्लिकेशन को चलाने के द्वारा, आप जांच कर सकते हैं कि क्या और कैसे वेबसाइटों अवरुद्ध कर रहे हैं, और सेंसरशिप और / या निगरानी प्रणालियों अपने नेटवर्क में मौजूद हैं।";
"risks_text_3" = "तुम भी गति और अपने नेटवर्क के प्रदर्शन को मापने के लिए इस अनुप्रयोग चला सकते हैं।";
"risks_text_4" = "डेटा जो आप इस ऐप के माध्यम से एकत्र कर सकते हैं वह सेंसरशिप की घटनाओं के साक्ष्य के रूप में काम कर सकता है।";
"risks_text_5" = "ओनोइप्रोब एक जांच उपकरण है और कुछ जोखिम पैदा कर सकता है।";

"potential_risks" = "संभाव्य जोखिम";
"laws_text_1" = "किसी को भी अपने इंटरनेट गतिविधि की निगरानी (उदाहरण के लिए आईएसपी, सरकार, नियोक्ता) पता कर सकते हैं कि आप यूनीप्रोबे (भले ही ऊनी इस कठिन बनाने के लिए कोशिश करता है) चल रहे हैं।";
"laws_text_2" = "सेंसरशिप के लिए वेबसाइटों का परीक्षण है, तो आप से कनेक्ट करने और उत्तेजक या आपत्तिजनक साइटों (जैसे पोर्नोग्राफी) है कि अपने देश में अवैध रूप से हो सकता है से डेटा डाउनलोड करेगा।";
"laws_text_3" = "ओनी के HTTP अमान्य अनुरोध पंक्ति परीक्षण आपके आईएसपी के संदेह को ट्रिगर कर सकते हैं और \"हैकिंग\" का एक रूप के रूप में देखा जा सकता है।";
"laws_text_4" = "जब तक आप अपनी सेटिंग्स को अन्यथा कॉन्फ़िगर न करें, आपके परीक्षणों से एकत्र किए गए सभी डेटा डिफ़ॉल्ट रूप से ऊनी  द्वारा प्रकाशित किए जाएंगे और इसमें कुछ व्यक्तिगत रूप से पहचाने जाने योग्य जानकारी शामिल हो सकती है (हालांकि ऊनी  इस होने से रोकने के लिए कोशिश करता है)।\n";
"laws_text_5" = "ooniprobe की मात्र उपयोग अपने देश में कानूनों की परवाह किए बिना, जासूसी के एक फार्म के रूप में देखा जा सकता है।";
"laws_text_6" = "हमारे ज्ञान करने के लिए, कोई यूनीप्रोबे उपयोगकर्ता कभी हमारे मंच के उपयोग से परिणामों का सामना किया है। हालांकि, कई देशों को निशाना बनाने और डिजिटल अधिकार कार्यकर्ताओं, जो यूनीप्रोबे उन गंभीर, दीवानी, आपराधिक, या अतिरिक्त न्यायिक दंड के अधीन छोड़ सकता कोस का एक लंबा इतिहास है।";
"laws_text_6" = "हमारे ज्ञान करने के लिए, कोई यूनीप्रोबे उपयोगकर्ता कभी हमारे मंच के उपयोग से परिणामों का सामना किया है। हालांकि, कई देशों को निशाना बनाने और डिजिटल अधिकार कार्यकर्ताओं, जो यूनीप्रोबे उन गंभीर, दीवानी, आपराधिक, या अतिरिक्त न्यायिक दंड के अधीन छोड़ सकता कोस का एक लंबा इतिहास है।";
